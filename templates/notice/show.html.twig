{% extends 'base.html.twig' %}

{% block title %}Notice{% endblock %}

{% block body %}
    <h1>Notice</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ notice.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ notice.name }}</td>
            </tr>
            <tr>
                <th>Comment</th>
                <td>{{ notice.comment }}</td>
            </tr>
            <tr>
                <th>Note</th>
                <td>{{ notice.note }}</td>
            </tr>
            <tr>
                <th>State</th>
                <td>{{ notice.state }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_notice_index') }}">back to list</a>

    <a href="{{ path('app_notice_edit', {'id': notice.id}) }}">edit</a>

    {{ include('notice/_delete_form.html.twig') }}
{% endblock %}
